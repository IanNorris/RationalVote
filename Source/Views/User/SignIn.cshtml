@{
	ViewBag.Title = "Sign in";
}

<div class="container-fluid">
	<div class="row-fluid col-wrap">
		<div class="col-md-4 col-md-offset-2">
			@Html.Partial( "Login" )
		</div>

		<div class="col-md-4">
			@Html.Partial( "Register" )
		</div>
	</div>
</div>

@section Scripts {
	@Scripts.Render( "~/bundles/jqueryval" )
	@Scripts.Render( "~/bundles/zxcvbn" )
}
